{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/api/airports/route.js",
    "app/Providers.js"
, "app/components/(auth)/Fields/Google.js", "app/components/(auth)/Fields/Form.js", "app/components/(auth)/Fields/Form.js", "app/api/auth/register/route.js", "app/api/auth/userExits/route.js", "app/api/auth/[...nextauth]/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
